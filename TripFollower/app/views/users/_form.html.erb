<%= form_for( @user, :html => { :class => "form-signin"} ) do |f| %>
  <h2 class="form-signin-heading"> <%= aliens %> </h2>
  <%= render "shared/errors" %>
  <br/>
  <%= f.label :fname, "First Name" %>
  <%= f.text_field :fname, :class => "input-block-level" %>
  <br/>
  <%= f.label :lname, "Last Name" %>
  <%= f.text_field :lname, :class => "input-block-level" %>
  <br/>
  <%= f.label :email, "Email" %>
  <%= f.text_field :email, :class => "input-block-level" %>
  <br/>
  <%= f.label :password, "Password" %>
  <%= f.password_field :password, :class => "input-block-level" %>
  <br />
  <%= f.label :password_confirmation, "Password Confirmation" %>
  <%= f.password_field :password_confirmation, :class => "input-block-level" %>
  <br/>
  <%= f.submit "submit", :class => "btn btn-large btn-primary" %>

<% end %>